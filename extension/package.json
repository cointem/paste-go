{
  "name": "paste-go",
  "displayName": "Paste Go",
  "publisher": "cointem",
  "description": "Smart paste for VS Code: convert JSON/SQL/code or text into typed structures with local parsing + AI fallback.",
  "version": "0.0.6",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cointem/paste-go"
  },
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": ["paste", "go", "smart paste", "ai", "code generation"],
  "icon": "icon.png",
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "paste-go.smartPaste",
        "title": "Smart Paste (Struct)",
        "category": "Paste Go"
      }
    ],
    "keybindings": [
      {
        "command": "paste-go.smartPaste",
        "key": "ctrl+alt+v",
        "mac": "cmd+alt+v",
        "when": "editorTextFocus"
      }
    ],
    "configuration": {
      "title": "Paste Go",
      "properties": {
        "pasteGo.corePath": {
          "type": "string",
          "default": "",
          "description": "Path to the Paste Go Go binary. If empty, tries to find in extension folder."
        },
        "pasteGo.aiApiKey": {
          "type": "string",
          "default": "",
          "description": "API Key for AI Fallback."
        },
        "pasteGo.aiApiFormat": {
          "type": "string",
          "default": "gemini",
          "description": "AI API format to use for fallback (e.g., gemini, openai).",
          "enum": [
            "gemini",
            "openai"
          ]
        },
        "pasteGo.aiModel": {
          "type": "string",
          "default": "",
          "description": "Specific model name (e.g., 'gpt-4o', 'gemini-1.5-pro'). Leave empty for default."
        },
        "pasteGo.aiBaseUrl": {
          "type": "string",
          "default": "",
          "description": "Base URL for OpenAI-compatible providers (e.g. DeepSeek)."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -b",
    "watch": "tsc -b -w",
    "package": "vsce package",
    "test": "vitest"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "@types/node": "^18.0.0",
    "typescript": "^5.0.0",
    "ts-loader": "^9.4.0",
    "webpack": "^5.0.0",
    "webpack-cli": "^5.0.0",
    "@vscode/vsce": "2.22.0"
  },
  "overrides": {
    "undici": "^5.28.0"
  }
}
